<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<!-- 个人中心 -->
	<package name="appGrzx" namespace="/app/grzx" extends="statistics-default">
		<!-- 账号覆盖率 -->
		<action name="grzx_usercover_*" class="grzxUsercoverAction"
			method="{1}">
			<result name="success">/WEB-INF/pages/app/grzx/usercover.jsp</result>
		</action>
		
		<!-- 新增来源 -->
		<action name="grzx_newfrom_*" class="grzxNewfromAction"
			method="{1}">
			<result name="success">/WEB-INF/pages/app/grzx/newfrom.jsp</result>
		</action>
		
		<!-- 注册来源 -->
		<action name="grzx_registerfrom_*" class="grzxRegisterfromAction"
			method="{1}">
			<result name="success">/WEB-INF/pages/app/grzx/registerfrom.jsp</result>
		</action>
	</package>
</struts>